{
  "name": "mmseqs-app",
  "productName": "MMseqs2 App",
  "version": "0.0.0",
  "author": "Milot Mirdita <milot@mirdita.de>",
  "description": "Frontend for the MMseqs2 search server and app.",
  "repository": "https://github.com/soedinglab/MMseqs2-App",
  "license": "GPL-3.0-or-later",
  "main": "dist/main.js",
  "scripts": {
    "electron:deps": "make all",
    "electron:build": "npm run electron:deps && cross-env ELECTRON=1 node ./electron/build.js && electron-builder --x64 -mwl",
    "electron:dir": "npm run electron:deps && cross-env ELECTRON=1 node ./electron/build.js && electron-builder --x64 --dir",
    "electron:dev": "cross-env ELECTRON=1 node ./electron/dev-runner.js",
    "electron:pack": "npm run electron:main && cross-env ELECTRON=1 npm run frontend",
    "electron:main": "webpack -p --progress --hide-modules --colors --config ./webpack.electron.config.js",
    "frontend": "webpack -p --progress --hide-modules  --colors --config ./webpack.frontend.config.js",
    "frontend:watch": "webpack --mode development --watch --progress --hide-modules --colors --config ./webpack.frontend.config.js",
    "frontend:dev": "webpack-dev-server --mode development --open --inline --hot --content-base dist/ --config ./webpack.frontend.config.js",
    "frontend:webpack-profile": "webpack -p --profile --json --config ./webpack.frontend.config.js > dist/webpack-stats.json",
    "api": "raml2html --template docs/api/theme/index.nunjucks docs/api/api.raml > docs/api.html"
  },
  "configuration": {
    "apiEndpoint": "/"
  },
  "build": {
    "productName": "MMseqs2 Search",
    "appId": "com.mmseqs.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "extraFiles": [
      {
        "from": "resources/${os}",
        "to": "Resources/bin"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "resources/icons/icon.icns"
    },
    "win": {
      "icon": "resources/icons/icon.ico"
    },
    "linux": {
      "icon": "resources/icons/icon.icns",
      "category": "Science"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "app-root-dir": "^1.0.2",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.2.4",
    "electron-builder": "^20.43.0",
    "electron-debug": "^2.2.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^3.0.1",
    "find-free-port": "=1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "icon-gen": "^2.0.0",
    "material-design-icons": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nunjucks": "^3.2.0",
    "raml2html": "^7.4.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg2png": "^4.1.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-localstorage": "^0.6.2",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuedraggable": "^2.21.0",
    "vuetify": "^1.5.14",
    "vuetify-loader": "^1.2.2",
    "webapp-webpack-plugin": "^2.7.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-subresource-integrity": "^1.3.2"
  }
}
