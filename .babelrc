{
    "comments": false,
    "presets": [
        [ "es2015", { "modules": false  } ],
        [ "stage-2" ]
    ],
    "plugins": [
        "transform-runtime",
        "add-filehash",
        [
            "transform-imports", {
                "vuetify": {
                    "transform": "vuetify/es5/components/${member}",
                    "preventFullImport": true
                }
            }
        ]
    ]
}